
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# KIOSK CONFIGURATION
# Unique identifier for this kiosk 
KIOSK_ID=kiosk-001

# SYNC INTERVAL (in seconds)
# How often the sync agent checks for updates
# Default: 60 (1 minute)
SYNC_INTERVAL=60

# CLINIC SOCKET URL
# Socket.IO URL for the clinic web app to receive emergency alerts
# Example: http://localhost:4000 or https://clinic.example.com
CLINIC_SOCKET_URL=http://localhost:4000

# ESP32 Serial Configuration
ESP32_SERIAL_PORT=/dev/ttyUSB0
ESP32_BAUD_RATE=115200
ESP32_ENABLED=true
