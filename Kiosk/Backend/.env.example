
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# KIOSK CONFIGURATION
# Unique identifier for this kiosk 
KIOSK_ID=kiosk-001

# FRONTEND API URL
# URL that the tablet (frontend) uses to reach the backend
# Examples: http://192.168.42.18:3001 (fixed IP) or http://kiosk-backend:3001 (docker network)
VITE_API_URL=http://192.168.42.18:3001

# Example: http://localhost:4000 or https://clinic.example.com
CLINIC_SOCKET_URL=http://localhost:4000

# ESP32 Serial Configuration
ESP32_SERIAL_PORT=/dev/ttyUSB0
ESP32_BAUD_RATE=115200
ESP32_ENABLED=true
